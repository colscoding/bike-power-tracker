{
    "name": "inspect",
    "version": "1.0.0",
    "description": "Inspection tools for Bike Power Tracker",
    "type": "module",
    "scripts": {
        "dev": "tsx src/server.ts",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "inspect:health": "tsx src/cli/check-health.ts",
        "inspect:streams": "tsx src/cli/list-streams.ts",
        "inspect:db": "tsx src/cli/inspect-db.ts",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "dotenv": "^16.4.5",
        "express": "^4.21.2",
        "pg": "^8.11.3",
        "redis": "^4.6.13"
