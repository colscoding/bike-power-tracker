    "devDependencies": {
        "@types/express": "^5.0.6",
        "@types/node": "^20.11.24",
        "@types/pg": "^8.11.2",
        "@types/redis": "^4.0.11",
        "autoprefixer": "^10.4.18",
        "postcss": "^8.4.35",
        "tailwindcss": "^3.4.1",
        "tsx": "^4.7.1",
        "typescript": "^5.3.3",
        "vite": "^5.1.4"
    }
